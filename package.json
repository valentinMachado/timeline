  "dependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.7.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.5.1",
    "string-replace-middleware": "^1.0.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
